{
	"name": "vite-plugin-php",
	"version": "1.0.20",
	"description": "Process PHP-files with the speed and tools of Vite",
	"keywords": [
		"vite",
		"vite-plugin",
		"load php",
		"use php",
		"vite php",
		"compile php",
		"php",
		"php-loader",
		"php-compiler",
		"loader",
		"url rewrite",
		"url router",
		"mod_rewrite"
	],
	"author": "Nikita 'donnikitos' Nitichevski <me@donnikitos.com> (https://donnikitos.com/)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/donnikitos/vite-plugin-php/issues"
	},
	"homepage": "https://github.com/donnikitos/vite-plugin-php#readme",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/donnikitos/vite-plugin-php.git"
	},
	"files": [
		"dist"
	],
	"type": "module",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"types": "./dist/index.d.ts",
			"default": "./dist/index.mjs"
		}
	},
	"scripts": {
		"build": "npx unbuild",
		"prepack": "npm run build"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^20.12.7",
		"fast-glob": "^3.3.2",
		"typescript": "^5.4.5",
		"unbuild": "^2.0.0",
		"vite": "^5.2.8"
	}
}
